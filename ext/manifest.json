{
  "manifest_version": 2,
 
  "name": "Modify CSS and HTML",
  "version": "0.1.0",
  "description": "Extension",
 
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://www.messenger.com/*", "http://www.messenger.com/*"]
  }],

  "permissions": [
    "tabs",
    "http://www.messenger.com/*",
    "http://www.messenger.com/*"
  ]
 
}